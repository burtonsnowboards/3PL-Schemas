{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://schema.infor.com/json-schema/M33PLReturnDeliveryNotification.json",
	"title": "M33PLReturnDeliveryNotification",
	"description": "This is an return delivery notification sent to a 3PL from Burton Infor M3.",
	"type": "object",
	"required": [
		"ReturnDeliveryNotification"
	],
	"properties": {
		"ReturnDeliveryNotification": {
			"type": "object",
			"required": [
				"document_date_time",
				"return_header",
				"return_line"
			],
			"properties": {
				"document_date_time": {
					"type": "string",
					"maxLength": 29,
					"description": "date in yyyy-MM-ddThh:mm:sssz (ISO) format, local warehouse timezone. e.g. 2022-07-25T08:34:32.777-04:00."
				},
				"return_header": {
					"type": "object",
					"required": [
						"company",
						"warehouse",
						"receiving_number",
						"order_number",
						"customers_order_number",
						"order_type",
						"gross_weight"
					],
					"properties": {
						"company": {
							"type": "string",
							"maxLength": 3
						},
						"warehouse": {
							"type": "string",
							"maxLength": 3
						},
						"receiving_number": {
							"type": "string",
							"maxLength": 10
						},
						"order_number": {
							"type": "string",
							"maxLength": 10
						},
						"customers_order_number": {
							"type": "string",
							"maxLength": 20
						},
						"order_type": {
							"type": "string",
							"maxLength": 3
						},
						"gross_weight": {
							"type": "string",
							"maxLength": 17
						}
					}
				},
				"return_line": {
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"order_line_number",
							"quantity",
							"item_number",
							"item_number_EA13",
							"item_number_UPC",
							"country_of_origins"
						],
						"properties": {
							"order_line_number": {
								"type": "string",
								"maxLength": 5
							},
							"quantity": {
								"type": "string",
								"maxLength": 16
							},
							"item_number": {
								"type": "string",
								"maxLength": 15
							},
							"item_number_EA13": {
								"type": "string",
								"maxLength": 13
							},
							"item_number_UPC": {
								"type": "string",
								"maxLength": 12
							},
							"country_of_origins": {
								"type": "array",
								"items": {
									"type": "object",
									"required": [
										"country_of_origin",
										"quantity_picked",
										"vendor",
										"manufacturer",
										"MID_code"
									],
									"properties": {
										"country_of_origin": {
											"type": "string",
											"maxLength": 3
										},
										"quantity_picked": {
											"type": "string",
											"maxLength": 17
										},
										"vendor": {
											"type": "string",
											"maxLength": 10
										},
										"manufacturer": {
											"type": "string",
											"maxLength": 10
										},
										"MID_code": {
											"type": "string",
											"maxLength": 20
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
